<div class="feedback-tool">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">Tool Affected</h3>
        </div>
        <div class="panel-body">
            <div class="radio-inline">
                <label>
                    <input type="radio" name="ToolOption" class="tool-option" value="none" checked />
                    None
                </label>
            </div>
            <div class="radio-inline">
                <label>
                    <input type="radio" name="ToolOption" class="tool-option" value="specific" />
                    Specific Tool:
                </label>
            </div>
            <div class="form-inline" style="display: inline-block;">
                <div class="form-group">
                    <select class="form-control proctech" style="width: 200px;" disabled></select>
                </div>
                <div class="form-group">
                    <select name="ResourceID" id="ResourceID" class="form-control resource" style="width: 400px;" disabled></select>
                </div>
            </div>
        </div>
    </div>
</div>